/**
* this the main function folder
*/

#include "MeOrion.h"

// LED control pins, connected to 2 - 4 Decoder
#define LED_PIN0 = A0;
#define LED_PIN1 = A1;

// input
#define LDR_IN = A2;
#define IR_IN = A3;

void setup()
{
  pinMode(LED_PIN1, OUTPUT);
  pinMode(LED_PIN2, OUTPUT);
  pinMode(LDR_IN, INPUT);
  pinMode(IR_IN, INPUT);
  Serial.begin(9600)

}
